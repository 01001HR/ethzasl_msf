<launch>
    <node launch-prefix="gdb -ex run --args" name="msf_position_filter" pkg="msf_updates" type="position_sensor" clear_params="true" output="screen">
            <remap from="msf_core/hl_state_input" to="/bluebird/fcu/ekf_state_out" />
            <remap from="msf_core/correction" to="/bluebird/fcu/ekf_state_in"/>

	    <remap from="msf_core/position_input" to="/leica/position" />
	    	
	    <remap from="msf_core/imu_state_input" to="/auk/fcu/imu" /> <!--DEBUGGING WITH VICON DATA-->
            <remap from="msf_updates/transform_input" to="vicon/auk/auk" /> <!--DEBUGGING WITH VICON DATA-->

            <rosparam file="$(find msf_updates)/position_sensor_fix.yaml"/>
    </node>
</launch>

