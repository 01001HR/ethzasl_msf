<launch>
    <node  launch-prefix="gdb -ex run --args" name="msf_viconpos_sensor" pkg="msf_updates" type="msf_viconpos_sensor" clear_params="true" output="screen">
            <remap from="msf_core/imu_state_input" to="/auk/fcu/imu" />
            <remap from="msf_core/position_measurement" to="vicon/auk/auk" />

            <rosparam file="$(find msf_updates)/viconpos_sensor_fix.yaml"/>
    </node>
</launch>
